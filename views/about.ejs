<%- include('partials/header') %>
    <%- include('partials/nav') %>
    <h1>About Page</h1>
    <script src="/js/data.js"></script>
    <!-- Create a variable -->
    <% const marks = [10,39, 87, 26] %>
    <% let name = 'Project High Jump' %>
    <!-- Displaying output -->
    <p><%= name %></p>

    <!-- display marks above 50 as h1 elements -->
    <% for(let mark of marks){ %>

        <%   if( mark > 50){ %>
                <h1><%= mark %></h1>
        <% } %>

    <% } %>

    <h2><%= data %></h2>

    <table>
        <thead></thead>
        <tr>
            <th>id</th>
            <th>name</th>
        </tr>
        <tbody>
            
            <% for(let item of items){ %>
                
                <tr>
                    <td><%= item.id %></td>
                    <td><%= item.name %></td>
                </tr>

            <% } %>

        </tbody>
    </table>


<%- include('partials/footer') %>